<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9BC2F9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nullteam.gui.MainSceneController">
    <children>
        <Label layoutX="90.0" layoutY="26.0" text="Containers">
            <font>
                <Font name="Calibri Bold Italic" size="21.0" />
            </font>
        </Label>
        <ListView fx:id="containersList" layoutX="31.0" layoutY="54.0" prefHeight="327.0" prefWidth="213.0">
            <effect>
                <DropShadow />
            </effect>
        </ListView>
        <Label layoutX="364.0" layoutY="146.0" text="Provide the Number">
            <font>
                <Font name="Calibri Light Italic" size="17.0" />
            </font>
        </Label>
        <TextField layoutX="358.0" layoutY="175.0">
            <effect>
                <DropShadow />
            </effect>
        </TextField>
        <Button layoutX="332.0" layoutY="356.0" mnemonicParsing="false" onAction="#removeContainer" text="Apply" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
        </Button>
        <Button layoutX="472.0" layoutY="356.0" mnemonicParsing="false" onAction="#pressContainers" text="Go Back" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
        </Button>
        <Button layoutX="357.0" layoutY="39.0" mnemonicParsing="false" onAction="#tapToListContainers" prefHeight="27.0" prefWidth="150.0" text="Tap to See Availables" textFill="#162dc1">
            <font>
                <Font name="Calibri Light" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9BC2F9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nullteam.gui.MainSceneController">
    <children>
        <ListView fx:id="imagesList" layoutX="24.0" layoutY="42.0" prefHeight="315.0" prefWidth="352.0">
            <effect>
                <DropShadow />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ListView>
        <Label layoutX="128.0" layoutY="14.0" text="Current Images">
            <font>
                <Font name="Calibri Bold Italic" size="21.0" />
            </font>
        </Label>
        <Button layoutX="413.0" layoutY="42.0" mnemonicParsing="false" onAction="#tapToListImages" prefHeight="27.0" prefWidth="150.0" text="View Images" textFill="#162dc1">
            <font>
                <Font name="Calibri Light" size="14.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="415.0" layoutY="329.0" mnemonicParsing="false" onAction="#applyPull" text="Apply" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="499.0" layoutY="329.0" mnemonicParsing="false" onAction="#pressImages" text="Go Back" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
      <TextField fx:id="imageToPull" layoutX="414.0" layoutY="187.0">
         <effect>
            <DropShadow />
         </effect>
          <cursor>
              <Cursor fx:constant="HAND"/>
          </cursor>
      </TextField>
      <Label layoutX="442.0" layoutY="153.0" text="Image To Pull">
         <font>
            <Font name="Calibri Light Italic" size="17.0" />
         </font>
      </Label>
    </children>
</AnchorPane>

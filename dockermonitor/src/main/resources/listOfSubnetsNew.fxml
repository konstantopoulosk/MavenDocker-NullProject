<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9BC2F9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nullteam.gui.MainSceneController">
    <children>
        <ListView fx:id="subnetsList" onMouseClicked="#retrieveIdToStop" layoutX="14.0" layoutY="58.0" prefHeight="327.0" prefWidth="400.0">
            <effect>
                <DropShadow />
            </effect>
        </ListView>
        <Button layoutX="422.0" layoutY="129.0" mnemonicParsing="false" onAction="#tapToSeeSubnets" prefHeight="27.0" prefWidth="177.0" text="Tap here to see the subnets" textFill="#162dc1">
            <font>
                <Font name="Calibri Light" size="14.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="476.0" layoutY="222.0" mnemonicParsing="false" onAction="#closeNewWindow" text="Go Back" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Label layoutX="123.0" layoutY="31.0" text="Subnets of Container">
            <font>
                <Font name="Calibri Bold Italic" size="21.0" />
            </font>
        </Label>
    </children>
</AnchorPane>

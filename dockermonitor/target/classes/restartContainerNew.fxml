<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9BC2F9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nullteam.gui.MainSceneController">
    <children>
        <Label layoutX="242.0" layoutY="14.0" text="Containers">
            <font>
                <Font name="Calibri Bold Italic" size="21.0" />
            </font>
        </Label>
        <ListView fx:id="activeContainers" layoutX="36.0" layoutY="36.0" onMouseClicked="#retrieveIdToStop" prefHeight="310.0" prefWidth="507.0">
            <effect>
                <DropShadow />
            </effect>
        </ListView>
        <Button layoutX="36.0" layoutY="356.0" mnemonicParsing="false" onAction="#restartContainer" text="Apply" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
        </Button>
        <Button layoutX="472.0" layoutY="356.0" mnemonicParsing="false" onAction="#pressContainers" text="Go Back" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
        </Button>
        <Button layoutX="214.0" layoutY="356.0" mnemonicParsing="false" onAction="#tapToSeeActiveContainers" prefHeight="27.0" prefWidth="150.0" text="Tap to See Availables" textFill="#162dc1">
            <font>
                <Font name="Calibri Light" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>

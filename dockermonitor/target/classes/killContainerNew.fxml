<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9BC2F9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nullteam.gui.MainSceneController">
    <children>
        <Label layoutX="253.0" layoutY="14.0" text="Containers">
            <font>
                <Font name="Calibri Bold Italic" size="21.0" />
            </font>
        </Label>
        <ListView fx:id="activeContainers" layoutX="50.0" layoutY="45.0" onMouseClicked="#retrieveIdToStop" prefHeight="296.0" prefWidth="500.0">
            <effect>
                <DropShadow />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ListView>
        <Button layoutX="50.0" layoutY="356.0" mnemonicParsing="false" onAction="#killContainer" text="Apply" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="479.0" layoutY="356.0" mnemonicParsing="false" onAction="#pressContainers" text="Go Back" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="225.0" layoutY="356.0" mnemonicParsing="false" onAction="#tapToSeeActiveContainers" prefHeight="27.0" prefWidth="150.0" text="View Active Containers" textFill="#162dc1">
            <font>
                <Font name="Calibri Light" size="14.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </children>
</AnchorPane>

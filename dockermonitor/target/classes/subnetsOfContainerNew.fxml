<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #9BC2F9;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nullteam.gui.MainSceneController">
    <children>
        <Label layoutX="252.0" layoutY="9.0" text="Containers">
            <font>
                <Font name="Calibri Bold Italic" size="21.0" />
            </font>
        </Label>
        <ListView fx:id="activeContainers" layoutX="32.0" layoutY="36.0" onMouseClicked="#retrieveIdToStop" prefHeight="308.0" prefWidth="534.0">
            <effect>
                <DropShadow />
            </effect>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </ListView>
        <Button layoutX="32.0" layoutY="356.0" mnemonicParsing="false" onAction="#applyToSeeSubnets" text="Apply" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="495.0" layoutY="356.0" mnemonicParsing="false" onAction="#pressContainers" text="Go Back" textFill="#162dc1">
            <font>
                <Font name="Calibri Light Italic" size="15.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button layoutX="224.0" layoutY="356.0" mnemonicParsing="false" onAction="#tapToSeeActiveContainers" prefHeight="27.0" prefWidth="150.0" text="Tap to See Availables" textFill="#162dc1">
            <font>
                <Font name="Calibri Light" size="14.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </children>
</AnchorPane>
